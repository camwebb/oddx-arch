    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:ObjectProperty rdf:about="&terms;creator"/>
    


    <!-- http://purl.org/openddx/oddx.rdf#disease -->

    <owl:ObjectProperty rdf:about="&oddx;disease">
        <rdfs:range rdf:resource="&obo;DOID_4"/>
        <rdfs:domain rdf:resource="&oddx;DiseaseSymptomRecord"/>
        <owl:inverseOf rdf:resource="&oddx;hasSymptomRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#hasDiseaseRecord -->

    <owl:ObjectProperty rdf:about="&oddx;hasDiseaseRecord">
        <rdfs:domain rdf:resource="&obo;SYMP_0000462"/>
        <rdfs:range rdf:resource="&oddx;DiseaseSymptomRecord"/>
        <owl:inverseOf rdf:resource="&oddx;symptom"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#hasFinding -->

    <owl:ObjectProperty rdf:about="&oddx;hasFinding">
        <rdfs:comment>The simplest association of a disease with a finding (= symptom sensu lato)</rdfs:comment>
        <rdfs:domain rdf:resource="&oddx;Disease"/>
        <rdfs:range rdf:resource="&oddx;Finding"/>
        <owl:inverseOf rdf:resource="&oddx;isFindingOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#hasSymptomRecord -->

    <owl:ObjectProperty rdf:about="&oddx;hasSymptomRecord">
        <rdfs:domain rdf:resource="&obo;DOID_4"/>
        <rdfs:range rdf:resource="&oddx;DiseaseSymptomRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#inPatient -->

    <owl:ObjectProperty rdf:about="&oddx;inPatient">
        <rdfs:domain rdf:resource="&oddx;DiseaseSymptomRecord"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#isFindingOf -->

    <owl:ObjectProperty rdf:about="&oddx;isFindingOf">
        <rdfs:comment>The simplest association of a disease with a finding (= symptom sensu lato)</rdfs:comment>
        <rdfs:range rdf:resource="&oddx;Disease"/>
        <rdfs:domain rdf:resource="&oddx;Finding"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#reference -->

    <owl:ObjectProperty rdf:about="&oddx;reference">
        <rdfs:range rdf:resource="&bibo;Document"/>
        <rdfs:domain rdf:resource="&oddx;DiseaseSymptomRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#symptom -->

    <owl:ObjectProperty rdf:about="&oddx;symptom">
        <rdfs:domain rdf:resource="&oddx;DiseaseSymptomRecord"/>
        <rdfs:range rdf:resource="&oddx;Finding"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/created -->

    <owl:DatatypeProperty rdf:about="&terms;created"/>
    


    <!-- http://purl.org/openddx/oddx.rdf#datePresent -->

    <owl:DatatypeProperty rdf:about="&oddx;datePresent">
        <rdfs:comment>The date the disease and symptom were present in the patient</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#estProbOfDisease -->

    <owl:DatatypeProperty rdf:about="&oddx;estProbOfDisease">
        <rdfs:comment>Estimated probability of the number of cases with an observed symptom in which a disease was found</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#estProbOfSymptom -->

    <owl:DatatypeProperty rdf:about="&oddx;estProbOfSymptom">
        <rdfs:comment>Estimated probability of the number of cases of a disease in which a symptom was observed</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/openddx/oddx.rdf#population -->

    <owl:DatatypeProperty rdf:about="&oddx;population">
        <rdfs:comment>Description of the population the record is drawn from. This would optimally point to another ontology for ethnic/geographic populations.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/age -->

    <owl:DatatypeProperty rdf:about="&foaf;age"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.obolibrary.org/obo/DOID_4 -->

    <owl:Class rdf:about="&obo;DOID_4">
        <owl:equivalentClass rdf:resource="&oddx;Disease"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/SYMP_0000462 -->

    <owl:Class rdf:about="&obo;SYMP_0000462">
        <owl:equivalentClass rdf:resource="&oddx;Symptom"/>
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Document -->

    <owl:Class rdf:about="&bibo;Document"/>
    


    <!-- http://purl.org/openddx/oddx.rdf#DiagnosticTest -->

    <owl:Class rdf:about="&oddx;DiagnosticTest">
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx.rdf#Disease -->

    <owl:Class rdf:about="&oddx;Disease"/>
    


    <!-- http://purl.org/openddx/oddx.rdf#DiseaseSymptomRecord -->

    <owl:Class rdf:about="&oddx;DiseaseSymptomRecord">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&oddx;hasDiseaseRecord"/>
                <owl:onClass rdf:resource="&oddx;Disease"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The core record/datum/observation associating a disease with a symptom. Can be either an observation of a single disease in a patient, or an association of a symptom with a disease from a publication.  Should only have a single associated disease, but may have multiple associated symptoms.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx.rdf#Finding -->

    <owl:Class rdf:about="&oddx;Finding">
        <rdfs:comment>The general class of findings about a disease in a patient, may include lab tests, symptoms (sensu stricto), signs (sensu stricto).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx.rdf#Sign -->

    <owl:Class rdf:about="&oddx;Sign">
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx.rdf#Symptom -->

    <owl:Class rdf:about="&oddx;Symptom">
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="&foaf;Person"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/openddx/oddx.rdf#Article0 -->

    <owl:NamedIndividual rdf:about="&oddx;Article0">
        <rdf:type rdf:resource="&bibo;Document"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/openddx/oddx.rdf#DSR0 -->

    <owl:NamedIndividual rdf:about="&oddx;DSR0">
        <rdf:type rdf:resource="&oddx;DiseaseSymptomRecord"/>
        <oddx:estProbOfSymptom>0.3</oddx:estProbOfSymptom>
        <oddx:datePresent>2011-01-01</oddx:datePresent>
        <rdfs:comment>An example instance to demonstrate usage</rdfs:comment>
        <terms:created>2012-03-14</terms:created>
        <oddx:estProbOfDisease>0.2</oddx:estProbOfDisease>
        <oddx:population>West Kalimantan, Indonesia, mixed ethnic</oddx:population>
        <oddx:reference rdf:resource="&oddx;Article0"/>
        <oddx:disease rdf:resource="&oddx;Disease1"/>
        <terms:creator rdf:resource="&oddx;Doctor0"/>
        <oddx:inPatient rdf:resource="&oddx;Patient0"/>
        <oddx:symptom rdf:resource="&oddx;Symptom1"/>
        <oddx:symptom rdf:resource="&oddx;Symptom2"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/openddx/oddx.rdf#Disease1 -->

    <owl:NamedIndividual rdf:about="&oddx;Disease1">
        <rdf:type rdf:resource="&obo;DOID_4"/>
        <oddx:hasFinding rdf:resource="&oddx;Symptom1"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/openddx/oddx.rdf#Doctor0 -->

    <foaf:Person rdf:about="&oddx;Doctor0">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
    </foaf:Person>
    


    <!-- http://purl.org/openddx/oddx.rdf#Patient0 -->

    <foaf:Person rdf:about="&oddx;Patient0">
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <oddx:population>location=West Kalimantan, ethnicity=Melayu</oddx:population>
        <foaf:age>23</foaf:age>
    </foaf:Person>
    


    <!-- http://purl.org/openddx/oddx.rdf#Symptom1 -->

    <owl:NamedIndividual rdf:about="&oddx;Symptom1">
        <rdf:type rdf:resource="&obo;SYMP_0000462"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/openddx/oddx.rdf#Symptom2 -->

    <owl:NamedIndividual rdf:about="&oddx;Symptom2">
        <rdf:type rdf:resource="&obo;SYMP_0000462"/>
    </owl:NamedIndividual>
</rdf:RDF>



